<idea-plugin>
    <id>com.lowjungxuan.proz</id>
    <name>ProZ</name>
    <vendor email="lowjungxuan@gmail.com.com" url="https://www.linkedin.com/in/low-jung-xuan-442b32222/">Low Jung Xuan
    </vendor>
    <description>
        <![CDATA[
    <h2>Simplify, Focus, Deliver â€“ That's ProZ</h2><ul><h3>Main Features</h3><li>GetX and Bloc Generator</li><li>GetX and Bloc Snippet</li><li>Json to Dart Generator</li><li>Flutter package's version checker</li></ul>
    ]]>
    </description>
    <depends>com.intellij.modules.platform</depends>
    <depends>org.jetbrains.plugins.yaml</depends>
    <depends>com.intellij.modules.lang</depends>
    <depends>com.intellij.modules.java</depends>
    <depends>Dart</depends>
    <depends>io.flutter</depends>
    <change-notes>
        Initial Upload
    </change-notes>
    <!-- Code Inspection Component  -->
    <extensions defaultExtensionNs="com.intellij">
        <externalAnnotator
                language="yaml"
                implementationClass="com.lowjungxuan.proz.versionchecker.annotator.PubPackagesAnnotator"/>
    </extensions>
    <!-- Settings -->
    <extensions defaultExtensionNs="com.intellij">
        <applicationService
                serviceImplementation="com.lowjungxuan.proz.versionchecker.settings.AppSettingsState"/>
    </extensions>
    <extensions defaultExtensionNs="com.intellij">
        <applicationConfigurable
                parentId="tools"
                instance="com.lowjungxuan.proz.versionchecker.settings.AppSettingsConfigurable"
                id="com.lowjungxuan.proz.versionchecker.settings.AppSettingsConfigurable"
                displayName="ProZ Setting"/>
    </extensions>
    <extensions defaultExtensionNs="com.intellij">
        <defaultLiveTemplates file="liveTemplates/GetX.xml"/>
        <liveTemplateContext id="FLUTTER" implementation="com.lowjungxuan.proz.newFolder.FlutterLiveTemplateContext"/>
    </extensions>
    <extensions defaultExtensionNs="com.intellij">
        <defaultLiveTemplates file="liveTemplates/Bloc.xml"/>
        <liveTemplateContext id="FLUTTER" implementation="com.lowjungxuan.proz.newFolder.FlutterLiveTemplateContext"/>
    </extensions>
    <actions>
        <action
                id="ProZ.GetX"
                class="com.lowjungxuan.proz.newFolder.NewFolderCreator"
                text="GetX Bloc"
                description="Create a new folder and Dart files."
                icon="com.lowjungxuan.proz.MyIcons.GetX">
            <add-to-group group-id="NewGroup" anchor="first"/>
        </action>
    </actions>
    <actions>
        <action id="DartJsonGenerator"
                class="com.lowjungxuan.proz.jsontodart.DartJsonNewFileAction"
                text="Dart Class from JSON"
                icon="com.lowjungxuan.proz.MyIcons.GetX">
            <add-to-group group-id="NewGroup" anchor="first"/>
        </action>
    </actions>
    <extensions defaultExtensionNs="com.intellij">
        <applicationService serviceImplementation="com.lowjungxuan.proz.settings.ProZSetting" />
        <projectConfigurable instance="com.lowjungxuan.proz.settings.ProZConfiguration" />
    </extensions>
    <extensions defaultExtensionNs="com.intellij">
        <!--Obx Action-->
        <intentionAction>
            <className>com.lowjungxuan.proz.newFolder.intention_action.WrapWithObxAction</className>
            <category>GetX</category>
        </intentionAction>
        <!--GetBuilder Action-->
        <intentionAction>
            <className>com.lowjungxuan.proz.newFolder.intention_action.WrapWithGetBuilderAction</className>
            <category>GetX</category>
        </intentionAction>
        <!--GetBuilder(Auto Dispose) Action-->
        <intentionAction>
            <className>com.lowjungxuan.proz.newFolder.intention_action.WrapWithGetBuilderAutoDisposeAction</className>
            <category>GetX</category>
        </intentionAction>
        <!--GetX Action-->
        <intentionAction>
            <className>com.lowjungxuan.proz.newFolder.intention_action.WrapWithGetXAction</className>
            <category>GetX</category>
        </intentionAction>
        <!--BlocBuilder Action-->
        <intentionAction>
            <className>com.lowjungxuan.proz.newFolder.intention_action.WrapWithBlocBuilderAction</className>
            <category>Provider</category>
        </intentionAction>
        <!--BlocConsumer Action-->
        <intentionAction>
            <className>com.lowjungxuan.proz.newFolder.intention_action.WrapWithBlocConsumerAction</className>
            <category>Provider</category>
        </intentionAction>
        <!--BlocListener Action-->
        <intentionAction>
            <className>com.lowjungxuan.proz.newFolder.intention_action.WrapWithBlocListenerAction</className>
            <category>Provider</category>
        </intentionAction>
        <!--BlocProvider Action-->
        <intentionAction>
            <className>com.lowjungxuan.proz.newFolder.intention_action.WrapWithBlocProviderAction</className>
            <category>Provider</category>
        </intentionAction>
        <!--RepositoryProvider Action-->
        <intentionAction>
            <className>com.lowjungxuan.proz.newFolder.intention_action.WrapWithRepositoryProviderAction</className>
            <category>Provider</category>
        </intentionAction>
    </extensions>
</idea-plugin>